{
  "name": "geist",
  "version": "1.0.0",
  "description": "Geist is a new font family for Vercel, created by Vercel in collaboration with Basement Studio.",
  "main": "./dist/font.js",
  "types": "./font.d.ts",
  "files": [
    "dist",
    "font.d.ts",
    ".images",
    "LICENSE.TXT"
  ],
  "exports": {
    "./font": {
      "import": "./dist/font.mjs",
      "require": "./dist/font.js"
    }
  },
  "bugs": {
    "url": "https://github.com/vercel/geist-font/issues"
  },
  "homepage": "https://vercel.com/font",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vercel/geist-font",
    "directory": "packages/next"
  },
  "keywords": [
    "geist",
    "geist mono",
    "geist sans",
    "vercel font"
  ],
  "scripts": {
    "prepublishOnly": "cp ../../LICENSE.TXT . && rm -rf ./dist && bunchee",
    "build": "bunchee"
  },
  "dependencies": {
    "@next/font": "^13.0.0 || ^14.0.0"
  },
  "devDependencies": {
    "bunchee": "^3.9.1"
  },
  "license": "SIL OPEN FONT LICENSE"
}
